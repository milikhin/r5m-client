<!DOCTYPE HTML>
<html>

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title>
    <% if(subtitle) { %>
      <%= subtitle %>.
    <% } %>
    <%= title.replace(/<br\/>/, ' ') %>
  </title>
  <link href="./dist/lp.css" rel="stylesheet" type="text/css" />
  <% for (var i=0; i<locals.fonts.google.length; i++) { %>
  <link href="<%= locals.fonts.google[i] %>" rel="stylesheet" type="text/css">
  <% } %>
</head>

<body>
  <% for(var i=0; i<locals.parts.length; i++) { %>
  <%- include('../../../tpl/partials/' + locals.parts[i]) %>
  <% } %>

  <script>
    window.r5m = {
      app: {
        // FEEDBACK_SERVICE: 'emailjs',
        FEEDBACK_SERVICE: '<%= feedbackService %>',
        FEEDBACK_EMAIL: '<%= feedbackEmail %>'
      },
      yandex: {
        COUNTER_ID: '<%= yandexId %>',
        DEFAULT_GOAL: 'r5m-call-me'
      },
      modules: ['menu-highlighter', 'feedback', 'r5mDimmer', 'carousel', 'gallery', 'page', 'utm-content'],
      utmContent: <%- JSON.stringify(locals.utmContent || {}) %>
    };
  </script>
  <script src="./dist/require.js"></script>

  <%- include('./client.ejs') %>
</body>

</html>
