<script>
  if(require && require.config){
    require.config({
      baseUrl: '/',
      paths: {
          text: "bower_components/text/text"
      },
      packages: [{
        "name": "dojo",
        "location" : "js/release/dojo",
        "main": "dojo"
      }, {
        "name": "dijit",
        "location" : "js/release/dijit",
        "main": "dijit"
      },{
        "name": "app",
        "location" : "js/app",
        "main": "index"
      }]
    });
  }




  require(['engine/lp'], function () {
    <% if(locals.startupScript) { %>
      require(["<%= locals.startupScript %>"]);
    <% } %>

    require(['r5m/app'], function (app) {
      app(window.r5m.modules);
    });
  });

  (function () {
    if (window.r5m.app.EMAILJS_ID) {
      window.emailjs.init(window.r5m.app.EMAILJS_ID);
    }
  })();
</script>
